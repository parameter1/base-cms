  "private": true,
  "scripts": {
    "lint:fix": "yarn lint --fix",
    "dev": "basecms-website dev --compile-dir ../../packages --watch-dir ../../packages --watch-ignore ../../packages/marko-web/integration --watch-ignore ../../packages/web-cli --purge-css-content-dir ../../packages",
    "build": "basecms-website build --compile-dir ../../packages",
    "build:css": "basecms-website build:css --purge-content-dir ../../packages",
    "build:js": "basecms-website build:js",
    "build:vite": "basecms-website build:vite",
    "build:ssr": "basecms-website build:ssr",
    "compile": "basecms-marko-compile compile",
    "lint": "eslint --ext .js --ext .vue --max-warnings 5 --ignore-path ../../.eslintignore ./",
    "test": "yarn lint && yarn compile --no-clean"
  },
  "dependencies": {
    "@parameter1/base-cms-marko-core": "^4.42.0",
