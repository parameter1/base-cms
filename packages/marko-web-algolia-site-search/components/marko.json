{
  "taglib-imports": [],
  "<marko-web-algolia-site-search-query>": {
    "template": "./query.marko",
    "@phrase": "string",
    "@limit": "number",
    "@skip": "number",
    "@section-ids": "array",
    "@supported-content-types": {
      "type": "object"
    },
    "@content-type": "string",
    "@country": "string",
    "@query-fragment": {
      "type": "string",
      "required": true
    },
    "@facet-filters": {
      "type": "array",
      "required": true
    }
  },
  "<marko-web-algolia-site-search-pagination-controls>": {
    "template": "./pagination-controls.marko",
    "@per-page": "number",
    "@total-count": "number",
    "@path": "string",
    "@query": "object"
  }
}
