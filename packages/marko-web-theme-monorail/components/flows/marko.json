{
  "taglib-imports": [
    "./content/marko.json"
  ],
  "<theme-card-deck-flow>": {
    "template": "./card-deck.marko",
    "@slots <slot>[]": {
      "@position": "number",
      "@index": "number",
      "@tag": "string",
      "@class": "string",
      "@modifiers": "array",
      "@attrs": "object"
    },
    "@nodes": "array",
    "@cols": "number",
    "@tag": "string",
    "@class": "string",
    "@modifiers": "array",
    "@attrs": "object"
  },
  "<theme-section-feed-flow>": {
    "template": "./section-feed.marko",
    "<header>": {},
    "<before>": {},
    "<after>": {},
    "<node>": {},
    "<node-list>": {},
    "@nodes": "array",
    "@display-image": "boolean",
    "@with-section": "boolean",
    "@with-sponsored": "boolean",
    "@lazyload": "boolean",
    "@modifiers": "array",
    "<native-x>": {
      "@index": "number",
      "@indexes": "array",
      "@name": "string",
      "@aliases": "array"
    },
    "@with-native-x-section": "boolean"
  }
}
